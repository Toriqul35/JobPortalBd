@model IEnumerable<Jobprotal.Models.SheekerAdd>
@{
    ViewBag.Title = "ViewSheekerAd";
    Layout = "~/Views/Shared/_All.cshtml";
}
<center><h2>View All Sheeker</h2></center>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>


        <th>
            @Html.DisplayNameFor(model => model.Phone_N)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Entry_Date)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone_N)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Entry_Date)
            </td>
            <td>
                @Html.ActionLink("Edit", "EditSheeker", new { id = item.She_Id }) |
                @Html.ActionLink("Delete", "DeleteSheeker", new { id = item.She_Id })
            </td>
        </tr>
    }
</table>
<div>
    <center><h2>@Html.ActionLink("Back to Home", "Index", "Admin")</h2></center>
</div> 